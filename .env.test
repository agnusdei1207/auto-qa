# Test Configuration
WEB_PORT=3001
BRAIN_PORT=9001
EXECUTOR_PORT=9002
DB_PORT=15433
OLLAMA_PORT=11435

# Ollama LLM
OLLAMA_API_URL=http://ollama:11434
LLM_MODEL=gemma3:1b

# Database Config
DB_HOST=database
DB_NAME=qa_results
DB_USER=qa_user
DB_PASS=qa_password

# File Persistence
SCREENSHOT_DIR=/tmp/screenshots
PROGRESS_DIR=/tmp/progress
ERROR_LOG_DIR=/tmp/error_logs

# Execution Modes
DEFAULT_HEADFUL=false
ENABLE_PARALLEL=true
MAX_PARALLEL_AGENTS=4

# Git Auto-Commit (disabled for testing)
ENABLE_GIT_AUTO_COMMIT=false
GIT_REPO_PATH=.
GIT_BRANCH=main

# Resource Monitoring
MAX_MEMORY_MB=4096
MAX_CPU_PERCENT=80
MONITOR_INTERVAL=5
